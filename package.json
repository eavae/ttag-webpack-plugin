{
  "name": "ttag-webpack-plugin",
  "version": "0.0.2",
  "description": "",
  "main": "dist/index.js",
  "engines": {
    "node": ">=6.x"
  },
  "scripts": {
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest tests-unit",
    "test:integration": "npm run build && sh tests-integration/before.sh && NODE_ENV=test:integration jest tests-integration",
    "update:fixtures:entry": "ttag update tests/fixtures/entry/entry.uk.po tests/fixtures/entry/",
    "webpack": "webpack --mode=development",
    "prettier": "prettier --write 'tests/**/*.js' 'plugin/**/*.js'",
    "build": "rm -rf dist && babel plugin --out-dir dist",
    "preversion": "npm run test",
    "prepublish": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "deepcopy": "^2.0.0"
  },
  "peerDependencies": {
    "babel-loader": ">=8.x",
    "webpack": ">=4.20.x",
    "babel-plugin-ttag": ">=1.7.17"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.6",
    "babel-plugin-ttag": "^1.7.15",
    "husky": "^2.4.0",
    "jest": "^24.8.0",
    "jest-puppeteer": "^4.2.0",
    "local-web-server": "^2.6.1",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "puppeteer": "^1.17.0",
    "tmp-promise": "^2.0.1",
    "ttag": "^1.7.15",
    "ttag-cli": "^1.7.15",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
